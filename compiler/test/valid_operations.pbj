master(map slaves, array args) {
	string a <- "abc";
	long b <- 10;
	boolean c <- true;
	c <- b = b;
	c <- b < |slaves| + b;
	double d <- 2.5;

	print(b + d);
	print(b - d);
	print(b * d);
	print(b / d);
	print(b % d);
	print(a = b);
	print(a === "abc");
	print(b > d);
	print(b >= d);
	print(b < d);
	print(b <= d);
	print(c || c);
	print(c && c);
}