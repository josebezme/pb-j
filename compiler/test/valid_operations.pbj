master(map slaves, array args) {
	string a <- "abc";
	long b <- 10;
	boolean c <- true;
	c <- b = b;
	c <- b < |slaves|;

	print(b + b);
	print(b - b);
	print(b * b);
	print(b / b);
	print(b % b);
	print(a = a);
	print(a === b);
	print(b > b);
	print(b >= b);
	print(b < b);
	print(b <= b);
	print(c || c);
	print(c && c);
}